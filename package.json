{
  "name": "sendgrid-typescript-project",
  "version": "1.0.0",
  "description": "A Node.js project with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "deploy": "gcloud functions deploy ocr-passport --runtime nodejs18 --trigger-event google.storage.object.finalize --trigger-resource your-bucket-name --entry-point processPassportImage",
    "deploy:notification": "gcloud functions deploy sendNotification --runtime nodejs18 --trigger-http --allow-unauthenticated --entry-point sendNotification"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "sendgrid",
    "ocr",
    "passport",
    "google-cloud",
    "fcm",
    "notifications"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/vision": "^4.0.2",
    "@google-cloud/firestore": "^7.1.0",
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/pubsub": "^4.1.0",
    "firebase-admin": "^12.0.0",
    "@sendgrid/mail": "^8.1.0"
  }
}
